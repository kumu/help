# jekyll and guard currently depend on conflicting versions
# of the listen gem. Until that's resolved we'll relax our
# dependency on guard.
source "https://rubygems.org"

gem "jekyll", "~> 1.4.2"
gem "jekyll-less"
gem "therubyracer" # required by less
gem "guard" # , "~> 2.2.5"
gem "guard-shell" # , "~> 0.5.2"
